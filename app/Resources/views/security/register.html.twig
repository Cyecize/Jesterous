{% extends 'base.html.twig' %}

{# @var localLang \AppBundle\Contracts\ILanguagePack #}
{% block title %}{{ localLang.websiteName }} - {{ localLang.register }}  {% endblock %}

{% block body %}

    {% embed 'partials/banner.html.twig' %}{% endembed %}
    <div class="page_content">
        <div class="container">
            <div class="row row-lg-eq-height">
                <div class="col-lg-9">
                    <div class="main_content">
                        {% embed 'partials/lang/'~ localLang.getLocalLang ~'/register.html.twig' %} {% endembed %}
                    </div>
                </div>
            </div>
        </div>
    </div>

{% endblock %}

{% block stylesheets %}
    <link rel="stylesheet" type="text/css" href="{{ asset('styles/styles/category.css') }}">
    <link rel="stylesheet" type="text/css" href="{{ asset('styles/styles/contact_responsive.css') }}">
{% endblock %}

{% block javascripts %}
    <script src="{{ asset('styles/js/custom.js') }}"></script>
{% endblock %}